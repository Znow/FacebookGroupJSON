<Window x:Class="FacebookGroupJSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Facebook Group JSON" Height="700" Width="600">
    <Grid Margin="0,0,-6.667,-8.333" RenderTransformOrigin="0.402,0.311">
        <ComboBox x:Name="ComboBox" HorizontalAlignment="Left" Margin="132,10,0,0" VerticalAlignment="Top" Width="254"/>
        <Button Content="Add New Feed" HorizontalAlignment="Left" Margin="449,10,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.133,1.057" Height="22" IsDefault="True" Click="Button_Click"/>
        <ListView x:Name="ListView" HorizontalAlignment="Left" Height="623" Margin="0,46,0,0" VerticalAlignment="Top" Width="590">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Titel" Width="400" DisplayMemberBinding="{Binding title}" />
                    <GridViewColumn Header="Dato" Width="100" DisplayMemberBinding="{Binding publishedDate}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
