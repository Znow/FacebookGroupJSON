<Window x:Class="FacebookGroupJSON.AddFeed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddFeed" Height="100" Width="300" Closing="Window_Closing">
    <Grid Margin="0,0,3.333,1.667">
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="205,28,0,0" VerticalAlignment="Top" Width="75" Height="22" Click="btnAdd_Click"/>
        <TextBox x:Name="txtSearchString" HorizontalAlignment="Left" Height="23" Margin="22,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="161"/>
        <Label Content="Search String" HorizontalAlignment="Left" Margin="64,2,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.026,-0.423"/>

    </Grid>
</Window>
